{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "mui-gamebook",
  "compatibility_date": "2025-11-20",
  "compatibility_flags": [
    // Enable Node.js API
    // see https://developers.cloudflare.com/workers/configuration/compatibility-flags/#nodejs-compatibility-flag
    "nodejs_compat",
    // Allow to fetch URLs in your app
    // see https://developers.cloudflare.com/workers/configuration/compatibility-flags/#global-fetch-strictly-public
    "global_fetch_strictly_public"
  ],
  "account_id": "fdc63eeea83ae8f5234357308b9a638b",
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "mui-gamebook",
      "database_id": "f1e05f17-9197-4344-a106-1fe0f906586f"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "placement": {
    "mode": "smart"
  },
  "r2_buckets": [
     {
       "binding": "NEXT_INC_CACHE_R2_BUCKET",
       "bucket_name": "baifo"
     },
     {
       "binding": "ASSETS_BUCKET",
       "bucket_name": "gamebook-assets"
     }
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      // The service should match the "name" of your worker
      "service": "mui-gamebook"
    }
  ],
  "vars": {
    "ASSETS_PUBLIC_DOMAIN": "https://i.muistory.com",
    "BETTER_AUTH_SECRET": "farewell",
    "GOOGLE_MODEL": "gemini-3-pro-preview",
    "GOOGLE_IMAGE_MODEL": "gemini-3-pro-image-preview",
    "NEXT_PUBLIC_SITE_URL": "https://muistory.com/"
  }
}
