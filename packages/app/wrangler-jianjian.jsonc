{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "xiaoniaoshuo-admin",
  "compatibility_date": "2025-11-20",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "account_id": "fdc63eeea83ae8f5234357308b9a638b",
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "xiaoniaoshuo-db",
      "database_id": "58b80611-f382-4e20-b110-cd50dac2f624"
    }
  ],
  "images": {
    "binding": "IMAGES"
  },
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "a9863dd8de514dc8940e0da4cf5c76da",
      "preview_id": "a9863dd8de514dc8940e0da4cf5c76da"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "placement": {
    "mode": "smart"
  },
  "r2_buckets": [
    {
      "binding": "NEXT_INC_CACHE_R2_BUCKET",
      "bucket_name": "site-cache"
    },
    {
      "binding": "ASSETS_BUCKET",
      "bucket_name": "xiaoniaoshuo"
    }
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      // The service should match the "name" of your worker
      "service": "xiaoniaoshuo-admin"
    }
  ],
  "vars": {
    "ASSETS_PUBLIC_DOMAIN": "https://i.xiaoniaoshuo.com",
    "COOKIE_DOMAIN": "xiaoniaoshuo.com",
    "HEADLESS_MODE": "true",
    "GOOGLE_MODEL": "gemini-3-pro-preview",
    "GOOGLE_IMAGE_MODEL": "gemini-2.5-flash-image",
    "GOOGLE_VIDEO_MODEL": "veo-3.1-fast-generate-preview",
    "NEXT_PUBLIC_GA_ID": "G-7GQKFPXHD1",
    "NEXT_PUBLIC_SITE_URL": "https://cms.xiaoniaoshuo.com",
    "ROOT_USER_EMAIL": "meathill@gmail.com",
    "RPC_PROVIDER_URL": "https://aeneid.storyrpc.io",
    "TRUSTED_ORIGINS": "https://xiaoniaoshuo.com"
  }
}
